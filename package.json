{
  "name": "@podlove/web-player-configurator",
  "version": "1.0.0",
  "description": "Podlove Web Player Configurator",
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "scripts": {
    "dist:clean": "rm -rf dist/*",
    "plugin:copy": "mkdir -p dist/ && cp -R src/plugin/* dist/ && cp -R node_modules/@podlove/web-player dist/",
    "plugin:dev": "nodemon --ext php --watch src/plugin --exec 'npm run plugin:copy'",
    "configurator:dev": "webpack --watch --config build/webpack.dev.conf.js",
    "configurator:build": "webpack --config build/webpack.prod.conf.js",
    "dev:init": "./wordpress dev init",
    "dev:save": "./wordpress dev down",
    "dev": "npm run dist:clean && concurrently 'npm run plugin:dev' 'npm run configurator:dev'",
    "build": "npm run dist:clean && npm run plugin:copy && npm run configurator:build"
  },
  "dependencies": {
    "element-ui": "2.13.0",
    "jquery": "3.4.1",
    "lodash": "4.17.15",
    "normalize.css": "8.0.1",
    "vue": "2.6.11",
    "vue-resource": "1.5.1",
    "vue-router": "3.1.3",
    "vuex": "3.1.2",
    "tailwindcss": "1.1.4",
    "vuex-router-sync": "5.0.0",
    "vuedraggable": "2.23.2",
    "@podlove/clients": "5.0.1-beta.1",
    "@podlove/web-player": "5.0.1-beta.1",
    "@podlove/player-actions": "5.0.1-beta.1",
    "object-hash": "2.0.1",
    "vue-prism-editor": "0.3.0",
    "prismjs": "1.18.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/plugin-transform-regenerator": "7.7.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.6.3",
    "@babel/runtime": "7.7.7",
    "babel-loader": "8.0.6",
    "concurrently": "5.0.2",
    "connect-history-api-fallback": "1.6.0",
    "copy": "0.3.2",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.0",
    "cssnano": "4.1.10",
    "eventsource-polyfill": "0.9.6",
    "expose-loader": "0.7.5",
    "express": "4.17.1",
    "file-loader": "5.0.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.20.0",
    "json-server": "0.15.1",
    "node-sass": "4.13.0",
    "nodemon": "2.0.2",
    "opn": "6.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "ora": "4.0.3",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "semver": "7.1.1",
    "shelljs": "0.8.3",
    "url-loader": "3.0.0",
    "vue-loader": "15.8.3",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.2.2",
    "postcss-clean": "1.1.0",
    "autoprefixer": "9.7.3",
    "postcss-loader": "3.0.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
