{
  "name": "@podlove/web-player-configurator",
  "version": "5.8.0",
  "description": "Podlove Web Player Configurator",
  "author": "Alexander Heimbuch <github@heimbu.ch>",
  "scripts": {
    "dist:clean": "rm -rf dist/*",
    "plugin:copy": "mkdir -p dist/ && cp -R src/plugin/* dist/ && cp -R node_modules/@podlove/web-player dist/",
    "plugin:dev": "nodemon --ext php --watch src/plugin --exec 'npm run plugin:copy'",
    "plugin:env": "cp ./env/.htaccess ./wp/.htaccess && cp ./env/wp-config.php ./wp/wp-config.php && mkdir -p ./wp/wp-content/mu-plugins && cp ./env/mu-plugins/* ./wp/wp-content/mu-plugins/*",
    "js:dev": "NODE_OPTIONS=--openssl-legacy-provider webpack --watch --config build/webpack.dev.conf.js",
    "js:build": "NODE_OPTIONS=--openssl-legacy-provider webpack --config build/webpack.prod.conf.js",
    "dev": "npm run dist:clean && concurrently 'npm run plugin:dev' 'npm run js:dev'",
    "build": "npm run dist:clean && npm run plugin:copy && npm run js:build",
    "package": "npm run build && cd dist && zip -r -X podlove-web-player-beta.zip .",
    "publish:prepare": "npm run build && cp -R ./src/assets ./dist/.wordpress-org && cp -R .git dist/ && cp .distignore dist/",
    "publish:workspace": "node build/workspace.js",
    "version": "node ./build/version.js",
    "release": "release-it"
  },
  "dependencies": {
    "@podlove/clients": "5.12.0",
    "@podlove/player-actions": "5.12.0",
    "@podlove/web-player": "5.13.0",
    "element-ui": "^2.15.14",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "normalize.css": "8.0.1",
    "object-hash": "2.0.1",
    "prismjs": "1.30.0",
    "shortid": "2.2.17",
    "tailwindcss": "1.9.6",
    "vue": "2.7.16",
    "vue-prism-editor": "0.3.0",
    "vue-resource": "^1.5.3",
    "vue-router": "3.1.3",
    "vuedraggable": "^2.24.3",
    "vuex": "3.1.2",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.6.0",
    "@babel/plugin-transform-react-jsx": "7.8.3",
    "@babel/plugin-transform-regenerator": "7.7.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.6.3",
    "@babel/runtime": "^7.28.4",
    "autoprefixer": "9.7.3",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "concurrently": "^5.3.0",
    "connect-history-api-fallback": "1.6.0",
    "copy": "0.3.2",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "3.4.0",
    "cssnano": "4.1.10",
    "eventsource-polyfill": "0.9.6",
    "expose-loader": "0.7.5",
    "express": "4.17.1",
    "file-loader": "5.0.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "0.20.0",
    "json-server": "0.15.1",
    "nodemon": "^2.0.22",
    "opn": "6.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "ora": "^4.1.1",
    "postcss-clean": "^1.2.2",
    "postcss-loader": "3.0.0",
    "release-it": "^19.0.5",
    "replace-in-file": "^6.3.5",
    "rimraf": "^3.0.2",
    "sass": "^1.93.2",
    "sass-loader": "10.5.1",
    "semver": "^7.7.3",
    "shelljs": "^0.8.5",
    "url-loader": "3.0.0",
    "vue-loader": "15.8.3",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "2.7.16",
    "webpack": "4.47.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-merge": "4.2.2"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "release-it": {
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "npm run version"
    }
  }
}
